{
  "name": "Vote",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false
    },
    "Stats": [
      {
        "method": "stats",
        "endpoint": "/stats",
        "description": "Statistical information for Vote registers.",
        "type": "model",
        "count": {
          "on": "createdAt",
          "by": "index"
        }
      }
    ]
  },
  "properties": {
    "value": {
      "type": [
        "number"
      ],
      "min": 1
    }
  },
  "validations": [],
  "relations": {
    "poll": {
      "type": "belongsTo",
      "model": "Poll"
    },
    "voter": {
      "type": "belongsTo",
      "model": "Member"
    }
  },
  "acls": [],
  "methods": {}
}
