{
  "name": "Block",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false
    },
    "Stats": [
      {
        "method": "stats",
        "endpoint": "/stats",
        "description": "Statistical information for Block registers.",
        "type": "model",
        "count": {
          "on": "createdAt",
          "by": "index"
        }
      }
    ]
  },
  "properties": {
    "type": {
      "type": "string",
      "required": true
    },
    "value": {
      "type": "any"
    }
  },
  "validations": [],
  "relations": {
    "blockContent": {
      "type": "belongsTo",
      "polymorphic": true
    },
    "topic": {
      "type": "belongsTo",
      "model": "Topic",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
